  "license": "MIT",
  "version": "1.0.8",
  "homepage": "https://github.com/wdmg/yii2-subscribers",
  "support": {
    "source": "https://github.com/wdmg/yii2-subscribers",
    "issues": "https://github.com/wdmg/yii2-subscribers/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.3.2",
    "wdmg/yii2-selectinput": "^1.0.9",
    "wdmg/yii2-helpers": "^1.4.4"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
